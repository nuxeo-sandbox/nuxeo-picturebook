<?xml version="1.0"?>
<component name="org.nuxeo.picturebook.slideshow.webresources">

  <require>org.nuxeo.web.ui.properties.contrib</require>

  <extension target="org.nuxeo.ecm.platform.WebResources" point="resources">
    <resource name="nuxeo-picturebook-webui-ui.html" type="import" shrinkable="false">
      <uri>/ui/nuxeo-picturebook-webui-ui.html</uri>
    </resource>
  </extension>

  <extension target="org.nuxeo.ecm.platform.WebResources" point="bundles">
    <bundle name="web-ui">
      <resources append="true">
        <resource>nuxeo-picturebook-webui-ui.html</resource>
      </resources>
    </bundle>
  </extension>
</component>
